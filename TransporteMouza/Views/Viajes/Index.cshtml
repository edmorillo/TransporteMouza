@model IEnumerable<TransporteMouza.Models.Viaje>

@{
    ViewData["Title"] = "Index";
}

<h1>Viajes</h1>

<p>
    <a asp-action="Create" class="btn btn-success">Create New</a>
</p>
<table class="table">
    <thead class="table-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Origen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Destino)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tarifa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Detalle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Remito)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumContenedor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdChoferesNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdClienteNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUnidadNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Origen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Destino)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tarifa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Detalle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Remito)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumContenedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdChoferesNavigation.IdChoferes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdClienteNavigation.IdCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUnidadNavigation.IdUnidad)
            </td>
            <td>
                    

                    <a asp-action="Edit" class="btn btn-primary" asp-route-id="@item.IdViajes">Edit</a> <br>
                    <a asp-action="Details" class="btn btn-primary" asp-route-id="@item.IdViajes">Details</a> <br>
                    <a asp-action="Delete" class="btn btn-danger" asp-route-id="@item.IdViajes">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
